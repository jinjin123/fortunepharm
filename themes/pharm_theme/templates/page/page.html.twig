<div class="layout-container page">
{# Header section #}
  <header id="header" class="navbar">
    <div class="container-fluid">
      <div class="container-fluid-40">
        {{ page.header }}
      </div>
    </div>
    <div class="fancybox-overlay" style="width: 100%; height: 1694px; display: none;">&nbsp;</div>
  </header>
{# Header section #}
{# Content section #}
  <div id="wrapper">
  <nav id="contact-nav" class="container-fluid">
    <div class="container-fluid-125">
      <div class="page_title">
          {{ page.page_title }}
      </div>
    </div>
  </nav>

  <div id="main">
    <div class="container-fluid">
      <section id="banner-wrapper">
      <div class="page_banner">
        {{ page.page_banner }}
      </div>
      </section>
      <section id="os-wrapper">
        {{ page.content }}
      </section>
      <section id="content_bottom">
        {{ page.content_bottom }}
      </section>
    </div>
  </div>
{# Content section #}
{# Footer section #}
  <footer class="footer" role="contentinfo" id="footer">
    <div class="container-fluid">
      <div class="container-fluid-40">
        {{ page.footer }}
      </div>
    </div>
  </footer>
</div>
</div>
{# Footer section #}
