<div class="layout-container page">
{# Header section #}
  <header id="header" class="navbar">
    <div class="container-fluid">
      <div class="container-fluid-40">
        {{ page.header }}
      </div>
    </div>
  </header>

{# Header section #}
{# Content section #}
  <div id="wrapper">
    <div rel="https://www.fortunepharm.com/" id="globalpath"></div>
    {% if page.page_title %}
    <nav id="contact-nav" class="container-fluid">

      <div class="container-fluid-125">
        <div class="page_title">
          {{ page.page_title }}
        </div>
      </div>
    </nav>
  {%endif  %}
  <div id="main">
    <div class="container-fluid">
      <section id="banner-wrapper">
      <div class="page_banner">
        {{ page.page_banner }}
      </div>
      </section>
      <nav id="contact-nav" class="container-fluid hidden-xs container-fluid-healthtips">
        <div class="container-healthtips-submenu">
          {{ page.sub_menu }}
        </div>
      </nav>
      <section id="os-wrapper">
      <div class="health-tips">
        <div id='health1'>
          <div id="health-tips" class="pain">
            <div id="wrapper">
              <div id="page-content-wrapper">
                <div id="main">
                  <div class="container-fluid">
                    <div class="content-outer-wrapper">
                      {% for item in node.field_healthtips_paragraph %}
                        <div>field_healthtips_details.value| check_markup(item.entity.field_healthtips_details.format)
                        </div>

                        <div class="notice-wrapper">
                          <p>
                            {{ item.entity.field_healthtips_short_descripti.value| check_markup(item.entity.field_healthtips_short_descripti.format) }}
                            <p></div>
                            <div>
                              item.entity.field_healthtips_body.value| check_markup(item.entity.field_healthtips_body.format)
                            </div>

                            {{ item.entity.field_healthtips_body.value| check_markup(item.entity.field_healthtips_body.format) }}
                            <div>
                              item.entity.field_healthtips_details.value| check_markup(item.entity.field_healthtips_details.format)
                            </div>
                            {{ item.entity.field_healthtips_details.value| check_markup(item.entity.field_healthtips_details.format) }}

                          {% endfor %}

                        </div>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>

      </section>
      <section id="content_bottom">
        {{ page.content_bottom }}
      </section>
    </div>
  </div>
{# Content section #}
{# Footer section #}
  <footer class="footer" role="contentinfo" id="footer">
    <div class="container-fluid">
      <div class="container-fluid-40">
        {{ page.footer }}
      </div>
    </div>
  </footer>
  </div>
</div>
{# Footer section #}
